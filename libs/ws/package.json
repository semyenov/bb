{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@regioni/lib-ws",
  "type": "module",
  "version": "0.0.2",
  "description": "WebSocket lib",
  "author": "Alexander Semyenov <semyenov@hotmail.com>",
  "license": "MIT",
  "homepage": "https://gitea.local/regioni/lib#readme",
  "repository": {
    "type": "git",
    "url": "git+https://gitea.local/regioni/lib.git"
  },
  "bugs": "https://gitea.local/regioni/lib/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./browser": {
      "types": "./dist/index.d.ts",
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll --minify",
    "dev": "yarn tsx src/index.ts",
    "typecheck": "yarn tsc --noEmit",
    "release": "yarn npm publish"
  },
  "dependencies": {
    "@regioni/lib-jose": "workspace:^",
    "@regioni/lib-logger": "workspace:^",
    "@types/ws": "^8.5.13",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "pkgroll": "^2.5.1",
    "typescript": "^5.7.2"
  }
}
