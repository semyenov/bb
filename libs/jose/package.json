{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@regioni/lib-jose",
  "type": "module",
  "version": "0.0.2",
  "description": "Rollup lib script",
  "author": "Alexander Semyenov <semyenov@hotmail.com>",
  "license": "MIT",
  "homepage": "https://gitea.local/regioni/lib#readme",
  "repository": {
    "type": "git",
    "url": "git+https://gitea.local/regioni/lib.git"
  },
  "bugs": "https://gitea.local/regioni/lib/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll --minify",
    "dev": "yarn tsx src/index.ts",
    "typecheck": "yarn tsc --noEmit",
    "release": "yarn npm publish"
  },
  "dependencies": {
    "@libp2p/crypto": "^5.0.8",
    "@regioni/lib-jose": "workspace:^",
    "@regioni/lib-logger": "workspace:^",
    "@regioni/orbit": "workspace:^",
    "bn": "^1.0.5",
    "bn.js": "^5.2.1",
    "consola": "^3.3.1",
    "elliptic": "^6.6.1",
    "jose": "^5.9.6"
  },
  "devDependencies": {
    "pkgroll": "^2.5.1",
    "typescript": "^5.7.2"
  }
}
