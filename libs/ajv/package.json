{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@regioni/lib-ajv",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "yarn@4.0.1",
  "description": "Rollup lib script",
  "author": "Alexander Semyenov <semyenov@hotmail.com>",
  "license": "MIT",
  "homepage": "https://gitea.local/regioni/lib#readme",
  "repository": {
    "type": "git",
    "url": "git+https://gitea.local/regioni/lib.git"
  },
  "bugs": "https://gitea.local/regioni/lib/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll --minify",
    "dev": "yarn tsx src/index.ts",
    "typecheck": "yarn tsc --noEmit",
    "release": "yarn npm publish"
  },
  "dependencies": {
    "@trpc/server": "^10.45.2",
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "ajv-i18n": "^4.2.0",
    "ajv-keywords": "^5.1.0",
    "consola": "^3.3.1",
    "fast-glob": "^3.3.2",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/events": "^3.0.3",
    "pkgroll": "^2.5.1",
    "typescript": "^5.7.2",
    "typescript-json-schema": "0.65.1"
  }
}
